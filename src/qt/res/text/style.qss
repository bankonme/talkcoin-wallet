
QLineEdit, QLineEdit:selected {
border-width: 1px;
border-color: white;
background-color: rgb(89,38,68);
alternate-background-color: rgb(103,47,82);
color: white;
selection-color: white;
}

QValidateLineEdit:hover, QLineEdit:hover {
background-color: rgb(103,47,82);
alternate-background-color: rgb(103,47,82);
color: black;
}

QLineEdit#txtChatNick,QLineEdit#txtChatMsg {
border-width: 2px;
border-color: rgb(154,154,154);
margin: 0px;
padding: 0px;
background-color: white;
alternate-background-color: white;
color: black;
}

QHeaderView {
font-size:11pt;font-family:'Lato';
alternate-background-color: rgb(103,47,82);
background-color: rgb(103,47,82);
color: #caa100;
}

QHeaderView::section:checked {
font-size:11pt;font-family:'Lato';
background-color: rgb(103,47,82);
color: #caa100 ;}

QTableView {
background: rgb(103,47,82);
color: white;
border-style: none;
alternate-background-color: rgb(89,38,68);
selection-background-color:QLinearGradient(x1: 0.8, y1: 0, x2: 1, y2: 0,stop: 0 rgb(88,0,118), stop: 1 rgb(41,0,100));
selection-color: white;
}

QHeaderView::section {
color:#caa100;
font-size:11pt;font-family:'Lato';
background-color: rgb(103,47,82);
selection-background-color: rgb(62,29,67);
}

QToolTip {
color: white;
font-size:9pt;font-family:'Lato';
border: 4px solid ;
border-radius: 13px;
opacity: 700;
border-color: rgb(103,47,82);
alternate-background-color: rgb(89,38,68);
background-color: rgb(62,29,67);
}

QDialog, QMainWindow, QFrame {
	color: white;
	background-color: rgb(62,29,67);
	alternate-background-color: rgb(89,38,68);
        border-width: -20px;
}

QLabel {
	background-color: transparent;
}

QCheckBox {
	color: white;
}

QFrame#central {
background-color: rgb(62,29,67);
border: none;
}
QFrame#topleftframe, QFrame#bottomleftframe, QFrame#middleframe,QFrame#bottomframe, QFrame#frame, QFrame#frame_3 {
	background-color: rgb(103,47,82);
	border-style: plain;
	border-width: 20px;
        border-radius: 25px;
	border-color: rgb(103,47,82);
	min-width: 10em;
	padding: 0px;
}
QFrame#toprightframe {
background-color: rgb(103,47,82);
border-style: plain;
border-width: 20px;
border-radius: 25px;
border-color: rgb(103,47,82);
min-width: 10em;
padding: 10px;
}

QFrame#messageframe, QFrame#txtframe {
border-style: plain;
border-width: 15px;
border-radius: 25px;
border-color: rgb(103,47,82);
margin: 0px;
min-width: 10em;
padding: 0px;
}

QListView#listtransactions {
border-width: 0px;
margin: 0px;
padding: 0px;
}

QLabel[openExternalLinks=true], QHeaderView { color: #cca000 }
QLabel[openExternalLinks=false] { color: white; font-weight: normal }

