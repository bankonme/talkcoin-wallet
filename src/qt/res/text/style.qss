QTabWidget {
	background: rgb(103,47,82);
	alternate-background-color: rgb(89,38,68);
        selection-background-color:QLinearGradient(x1: 0.8, y1: 0, x2: 1, y2: 0,stop: 0 rgb(88,0,118), stop: 1 rgb(41,0,100));
}

QListView {
	background: rgb(103,47,82);
	alternate-background-color: rgb(89,38,68);
}

 QTabWidget::pane {
	border-top: 2px solid rgb(103,47,82);
	border: 4px solid rgb(103,47,82);
	border-bottom-left-radius: 8px;
	border-bottom-right-radius: 8px;
	border-top-right-radius: 8px;
	background: rgb(103,47,82);
}

QTabWidget::tab {
	background: rgb(103,47,82);
	border: 4px solid rgb(62,29,67);
	border-bottom-color: rgb(103,47,82); /* same as the pane color */
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	min-width: 8ex;
	padding: 4px;
	color: #888;
}

QTabBar::tab:selected {
	color: #caa100;
}
QTabBar::tab:hover {
	color: #cca100;
}

QTabBar::tab:selected {
	border-color: rgb(103,47,82); /* same as pane color */
	border-top-color: #cca000;
}

QTabBar::tab:!selected {
	margin-top: 2px; /* make non-selected tabs look smaller */
}

QTabBar::tab:selected {
	margin-left: -4px;
	margin-right: -4px;
}

QTabBar::tab:first:selected {
	margin-left: 0;
}

QTabBar::tab:last:selected {
	margin-right: 0;
}

QTabBar::tab:only-one {
	margin: 0;
}
