#btnChatSend, #btnChatSmiley, #btnChatBold {
padding: 3px;
margin: 0px;
border-radius: 0px;
border-width: 1px;
border-color: white;
}

QPushButton, QToolButton {
	border: 4px solid rgb(103,47,82);
	border-radius: 12px;
	background-color: rgb(103,47,82);
	color: white;
	padding: 6px;
	margin: 6px;
}


QPushButton:hovered, QToolButton:hovered {
        background-color:QLinearGradient(x1: 0.8, y1: 0, x2: 1, y2: 0, stop: 0 rgb(88,0,118), stop: 1 rgb(41,0,100));
}
QPushButton:pressed, QToolButton:checked {
	color: #cca000;
}
QPushButton:pressed, QToolButton:pressed {
	border-color: #cca000;
        background-color: rgb(89,38,68);
	color: #cca000;
}
QPushButton:disabled, QToolButton:disabled {
	color: #444;
	background-color: rgb(62,29,67);
}


#scrollAreaWidgetContents QToolButton {
	border-radius: 8px;
	border-width: 0px;
}

QStatusBar { background: rgb(62,29,67); }


#scrollAreaWidgetContents { background: rgb(62,29,67); }
#scrollAreaWidgetContents > QFrame { background-color: rgb(62,29,67); border-width: 0px; border-style: none; }


QLineEdit#txtChatNick,QLineEdit#txtChatMsg {
border-radius: 0px;
border-width: 1px;
border-color: rgb(154;154;154);
margin: 1px;
padding: 0px;
background-color: white;
alternate-background-color: white;
color: black;
}

QComboBox {
	border-radius: 6px;
	border-width: 0;
	border-style: none;
	padding: 8px;
	background-color: rgb(103,47,82);
	color: white;
        font: 11pt "Lato";
}

QTextBrowser {
	border-radius: 6px;
	border-width: 0;
	border-style: none;
	padding: 8px;
        background-color: blue;
        alternate-background-color: rgb(103,47,82);
	color: black;
}

QComboBox::drop-down {
	border-width: 0;
	border-style: none;
	background-color: rgb(103,47,82);
	color: white;
}
